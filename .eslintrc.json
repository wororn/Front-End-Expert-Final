{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },

    "extends": [
        "airbnb-base"
    ],

    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "consistent-return": 0,
        "max-classes-per-file":0, 
        "no-undef":0,
        "no-use-before-define":0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "class-methods-use-this": 0,
        "no-new": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "eqeqeq":0,
        "no-param-reassign":0,
        "no-tabs":0,
        "func-names":0,
        "padded-blocks":0,
        "no-useless-escape":0,
        "no-mixed-spaces-and-tabs":0,
        "no-restricted-syntax":0,
        "guard-for-in":0,
        "max-len":0,
        "no-mixed-operators":0,
        "no-nested-ternary":0,
        "radix":0,
        "no-unused-vars":0    
        }

}
